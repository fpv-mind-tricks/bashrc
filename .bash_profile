# Setting PATH for Python 3.6
# The original version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/3.6/bin:${PATH}"
export PATH
alias c='cd'
alias ..='cd ../'
alias n='nvim'
alias l='ls -la'
alias t='tree -L 2 | less'
alias gcpp='rg -m 4 -tcpp'
alias f='find . -name'
